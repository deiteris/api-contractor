{
  "name": "vs-code-api-console",
  "description": "The API Console package for Visual Studio Code.",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Yury Palyanitsa"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deiteris/api-contractor.git"
  },
  "dependencies": {
    "api-console": "^6.4.10",
    "fs-extra": "^10.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.2",
    "@open-wc/eslint-config": "^4.3.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/fs-extra": "^9.0.11",
    "babel-plugin-bundled-import-meta": "^0.3.2",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^2.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "postcss": "^8.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-cpy": "^2.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "uglify-js": "^3.13.7"
  },
  "resolutions": {
    "postcss": "^8.3.0",
    "underscore": "^1.13.1",
    "node-fetch": "^2.6.1"
  },
  "scripts": {
    "postinstall": "node tasks/fix-package-lock.js",
    "start": "node demo/index.js",
    "lint:eslint": "eslint --ext .js,.html . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .js,.html . --fix --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "build": "rimraf dist && rollup -c rollup.config.js && node tasks/postbuild.js",
    "build:vendor": "node tasks/prepare.js",
    "build:full": "npm run build:vendor && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=12.0.0",
    "vscode": "^1.54.0"
  }
}
